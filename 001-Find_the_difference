class Solution {
public:
    char findTheDifference(string s, string t) {
        int n = s.size();
        int v =t.size();
        int XOR1 =0;
        int XOR2=0;
        for(int i=0;i<n;i++){
            XOR1= XOR1^s[i];
        }
        for(int i=0;i<v;i++){
            XOR2 = XOR2^t[i];
        }
        return XOR1^XOR2;
    }
};
